{
  "name": "meanjs-template",
  "description": "MeanJS Template",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development grunt",
    "build": "grunt build",
    "production": "NODE_ENV=production node server.js",
    "test": "grunt test",
    "test-server-watch": "NODE_ENV=test jasmine-node --test-dir test/server --matchall --color --autotest --watch app",
    "test-client-watch": "NODE_ENV=test ./node_modules/karma/bin/karma start karma.conf.js",
    "ci": "CI=true grunt ci",
    "update-webdriver": "./node_modules/.bin/webdriver-manager update",
    "protractor": "./node_modules/.bin/protractor protractor-conf.js",
    "postinstall": "npm rebuild; npm run update-webdriver; bower install --config.interactive=false --allow-root"
  },
  "dependencies": {
    "async": "~0.9.0",
    "body-parser": "~1.12.0",
    "chai": "~2.3.0",
    "compression": "~1.4.2",
    "consolidate": "~0.11.0",
    "cookie-parser": "~1.3.4",
    "cookie-session": "~1.1.0",
    "debug-caller": "~2.0.0",
    "express": "~4.12.2",
    "express-domain-middleware": "~0.1.0",
    "git-rev-sync": "~1.0.0",
    "glob": "~4.0.5",
    "helmet": "~0.7.0",
    "lodash": "~3.5.0",
    "mongoose": "~4.0.3",
    "morgan": "~1.5.1",
    "passport": "~0.2.1",
    "request": "~2.53.0",
    "swig": "~1.4.2",
    "uuid": "~2.0.1",
    "verror": "~1.6.0"
  },
  "devDependencies": {
    "autoprefixer-core": "~5.1.7",
    "bower": "~1.3.12",
    "breakpoint-sass": "~2.5.0",
    "grunt": "~0.4.5",
    "grunt-angular-templates": "~0.5.7",
    "grunt-cli": "~0.1.13",
    "grunt-concurrent": "~1.0.0",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-concat": "~0.5.1",
    "grunt-contrib-cssmin": "~0.12.2",
    "grunt-contrib-jshint": "~0.11.0",
    "grunt-contrib-uglify": "~0.8.0",
    "grunt-contrib-watch": "~0.6.1",
    "grunt-env": "~0.4.4",
    "grunt-jasmine-node": "~0.2.1",
    "grunt-karma": "~0.10.1",
    "grunt-nodemon": "~0.4.0",
    "grunt-postcss": "~0.3.0",
    "grunt-protractor-runner": "~2.0.0",
    "grunt-sass": "~0.18.1",
    "jasmine-node": "~1.14.5",
    "jasmine-reporters": "~1.0.1",
    "karma": "~0.12.31",
    "karma-chrome-launcher": "~0.1.7",
    "karma-firefox-launcher": "~0.1.4",
    "karma-jasmine": "~0.3.5",
    "karma-junit-reporter": "~0.2.2",
    "karma-phantomjs-launcher": "~0.1.4",
    "load-grunt-tasks": "~3.1.0",
    "nodemon": "~1.3.7",
    "protractor": "~2.0.0",
    "rewire": "~2.3.1",
    "susy": "^2.2.2-alpha.1"
  }
}
